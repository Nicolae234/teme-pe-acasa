n = int(input("Introduceți un număr n: "))
s1 = 0
for i in range(1, n + 1):
    s1 += i

s2 = 0
for i in range(1, n):
    s2 += i * (i + 1)
    
s3 = 1
produs = 1
for i in range(1, n + 1):
    produs *= i
    s3 += produs

s4 = 0
for i in range(1, n + 1):
    s4 += i**2

s5 = 0
for i in range(1, n + 1):
    s5 += i / (i + 1)

s6 = 0
for i in range(1, 2 * n + 1):
    s6 += int('2' * i)

print(s1)
print(s2)
print(s3)
print(s4)
print(s5)
print(s6)
